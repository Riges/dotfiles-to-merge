# Local/private config goes in the include
[include]
        path = ~/.gitconfig.local
[alias]
	ci = commit
   	co = checkout
   	st = status
   	br = branch
        promote = !$ZSH/bin/git-promote
        wtf     = !$ZSH/bin/git-wtf
        rank-contributors = !$ZSH/bin/git-rank-contributors
        count   = !git shortlog -sn
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
        editor = vim
